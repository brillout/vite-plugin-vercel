    "@vercel/edge-config": "^1.2.1",
    "@vercel/og": "^0.6.2",
    "@vite-plugin-vercel/vike": "workspace:*",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "cross-fetch": "^4.0.0",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.4.5",
    "vike": "^0.4.190",
    "vite": "^5.4.1",
    "vite-plugin-vercel": "workspace:*"
  },
  "type": "module",
  "version": null,
  "devDependencies": {
    "@types/node": "^18.19.45",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vercel/node": "^3.0.26",
    "fast-glob": "^3.3.2",
    "vitest": "^2.0.5",
    "zod": "^3.22.4"
  }
